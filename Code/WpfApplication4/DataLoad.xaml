            <Button x:Name="Button_Subsidies_Bills" Template="{DynamicResource ButtonControlTemplate_Normal}" Grid.Column="4" Grid.Row="1" Content="Subsidies &amp; Bills"  Height="45" Margin="0,2.5" Click="Button_Subsidies_Bills_Click"/>

        </Grid>
        <Image x:Name="Logo_png" Margin="10,7,0,0" Source="Logo.png" Stretch="Fill" Height="35" VerticalAlignment="Top" HorizontalAlignment="Left" Width="35"/>



    </Grid>
</Window>
